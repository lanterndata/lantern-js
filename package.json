{
  "name": "lantern",
  "version": "1.0.0",
  "description": "JavaScript/Typescript ORMs for Lantern Vector Database",
  "main": "index.js",
  "scripts": {
    "test": "node --test ./src/**/*.test.mjs",
    "prettier:format": "prettier . --write",
    "prettier:check": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lanterndata/lantern-node"
  },
  "keywords": [
    "lantern",
    "lanterndb",
    "vector",
    "vector-search",
    "postgres",
    "postgresql"
  ],
  "author": "nairihar",
  "license": "BSL",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./knex": {
      "types": "./src/knex/knex.d.ts",
      "default": "./src/knex/knex.js"
    },
    "./sequelize": {
      "types": "./src/sequelize/sequelize.d.ts",
      "default": "./src/sequelize/sequelize.js"
    },
    "./embeddings": {
      "types": "./src/_embeddings/embeddings.d.ts",
      "default": "./src/_embeddings/embeddings.js"
    },
    "./sql": {
      "types": "./src/_common/utils/sql.d.ts",
      "default": "./src/_common/utils/sql.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/index.d.ts"
      ],
      "knex": [
        "src/sequelize/knex.d.ts"
      ],
      "sequelize": [
        "src/sequelize/sequelize.d.ts"
      ],
      "embeddings": [
        "src/_embeddings/embeddings.d.ts"
      ],
      "sql": [
        "src/_common/utils/sql.d.ts"
      ]
    }
  },
  "devDependencies": {
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "prettier": "3.2.5",
    "sequelize": "^6.36.0"
  }
}
